@namespace("com.my.sibyl.itemsets.model")
protocol ItemSets {

  record Instance {
    string name;
    array<string> measures = [];
    array<string> dataLoadFiles = [];
    long startLoadDate;
    long endLoadDate;
    long slidingWindowSize;
  }

  record Transaction {
    string id;
    array<string> items = [];
    array<int> quantities = [];
    long createTimestamp;
  }
}
